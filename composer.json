{
  "name": "zfcampus/zf-doctrine-mapping",
  "type": "library",
  "description": "Mappers and QueryBuilder for Doctrine",
  "keywords": [
    "doctrine",
    "filter",
    "module",
    "zend",
    "zf2"
  ],
  "homepage": "https://github.com/zfcampus/zf-doctrine-mapping",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Julien Guittard",
      "email": "julien.guittard@me.com"
    }
  ],
  "require": {
    "php": "^5.6 | ^7.0",
    "doctrine/doctrine-orm-module": "dev-master",
    "container-interop/container-interop": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.5",
    "squizlabs/php_codesniffer": "^2.0",
    "behat/behat": "^3.1"
  },
  "suggest": {
    "ext/mongo": "Mongo extension, if using ODM",
    "doctrine/doctrine-mongo-odm-module": "v0.*, if you wish to use the Doctrine ODM",
    "doctrine/doctrine-orm-module": "v0.*, if you wish to use the Doctrine ORM"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "ZF\\Doctrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZF\\Doctrine\\Features\\": "features/bootstrap/",
      "ZFTest\\Doctrine\\": "test/"
    }
  },
  "extra": {
    "zf": {
      "module": "ZF\\Doctrine"
    },
    "branch-alias": {
      "dev-master": "1.0-dev",
      "dev-develop": "1.1-dev"
    }
  }
}
